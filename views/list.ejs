<%- include("header") -%>

    <div class="box" id="heading">
        <h1>
            <%= listStyle %>
        </h1>
    </div>
    <div class="box">
        <% for(var i=0; i<newItems.length; i++){ %>
        <div class="item">
            <input type="checkbox">
            <p> <%= newItems[i] %> </p> 
         </div>
         <% } %>

         <form action="/" method="post">
            <input type="text" name="item" placeholder="Add New Item" autocomplete="off">
            <button type="submit" name="list" value=<%= listStyle %>>+</button>
        </form>

    </div>

   
<%- include("footer") -%>